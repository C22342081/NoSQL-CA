{
  "Database schema": {
    "year": "int",                // The year of the data (e.g., 2016)
    "region": {                       // Embedded document for region information
    "name": "string"                  // Name of the region (e.g., "All Counties")
  },
  "water_consumption": {            // Embedded document for water consumption statistics
    "mean": {                       // Mean consumption data
      "label": "string",              // Description of the statistic
      "value": "int",        // Consumption value (e.g., 368.0)
      "unit": "string"                // Unit of measurement (e.g., "litres")
    },
    "median": {                     // Median consumption data
      "label": "string",              // Description of the statistic
      "value": "int",        
      "unit": "string"                
    }
  }
}
}